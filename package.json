{
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src\"",
    "start": "dotenv -c '.env' -- node dist/app",
    "start:prod": "node dist/app",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t $npm_package_name:local -f Dockerfile .",
    "docker:tag": "node -e \"console.log('No-op. Use docker:release instead.')\"",
    "docker:release": "bash ./docker-release.sh ${npm_package_version:-latest} ${DOCKER_REPO:-zevi/adminjs-demo}"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/prisma": "^5.0.4",
    "@prisma/client": "^5.6.0",
    "@tiptap/core": "^2.26.1",
    "@tiptap/extension-horizontal-rule": "^2.26.1",
    "adminjs": "^7.8.17",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "mysql2": "latest"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "name": "adminjs-demo",
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
